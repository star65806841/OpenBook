{
    "title": "%(title)s",
    "background": "%(background)s",
    "icon":"%(volume_icon)s",
    "icon-size":120,
    "format": "UDZO",
    "compression-level": 9,
    "window": { "position": { "x": 100, "y": 100 },
                "size": { "width": 540, "height": 360 }
    },
    "contents": [
        { "x": 60, "y": 165, "type": "file", "path": "%(app_path)s" },
        { "x": 380, "y": 165, "type": "link", "path": "/Applications" }
    ]
}
